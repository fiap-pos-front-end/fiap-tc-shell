{
  "name": "fiap-tc-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "ng serve --port 4200",
    "start:fiap-tc-react": "cd ../fiap-tc-react && npm start",
    "start:fiap-tc-angular": "cd ../fiap-tc-angular && npm start",
    "start:fiap-tc-angular2": "cd ../fiap-tc-angular2 && npm start",
    "start:fiap-tc-api": "cd ../fiap-tc-api && npm start",
    "install:fiap-tc-react": "cd ../fiap-tc-react && npm i",
    "install:fiap-tc-angular": "cd ../fiap-tc-angular && npm i",
    "install:fiap-tc-angular2": "cd ../fiap-tc-angular2 && npm i",
    "install:fiap-tc-api": "cd ../fiap-tc-api && npm i",
    "start:all": "concurrently \"cd ../fiap-tc-react && npm run dev\" \"npm run start:fiap-tc-angular\" \"npm run start:fiap-tc-angular2\" \"npm run start:fiap-tc-api\" \"npm run start\"",
    "install:all": "concurrently \"npm run install:fiap-tc-react\" \"npm run install:fiap-tc-angular\" \"npm run install:fiap-tc-angular2\" \"npm run install:fiap-tc-api\" \"npm i\"",
    "sync:all": "concurrently \"cd ../fiap-tc-angular && git checkout main && git pull\" \"cd ../fiap-tc-angular2 && git checkout main && git pull\" \"cd ../fiap-tc-react && git checkout main && git pull\" \"cd ../fiap-tc-shared && git checkout main && git pull\" \"cd ../fiap-tc-api && git checkout main && git pull\" \"cd ../fiap-tc-shell && git checkout main && git pull\""
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^19.0.3",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@fiap-pos-front-end/fiap-tc-shared": "^0.2.22",
    "@ngrx/signals": "^19.2.1",
    "@primeng/themes": "^19.1.3",
    "concurrently": "^8.2.1",
    "css-loader": "^7.1.2",
    "mitt": "^3.0.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primelocale": "^2.1.4",
    "primeng": "^19.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-build-plus": "^19.0.0",
    "postcss": "^8.4.31",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.35.1"
  }
}
